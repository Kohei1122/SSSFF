<div class="main users-index">
  <div class="container">
    <div class="users-heading"><h2>Users</h2></div>
    <% @users.each do |user| %>
      <div class="users-index-item">
        <div class="user-left">
          
        </div>
        <div class="user-right">
          <%= link_to(user.name, "/users/#{user.id}") %>
        </div>
      </div>
    <% end %>
  </div>
</div>